{
  "author": "Valentin Chassignol <contact@vinetos.fr> (https://www.vinetos.fr)",
  "bugs": {
    "url": "https://github.com/Vinetos/Vinetos.fr/issues"
  },
  "bundleDependencies": false,
  "deprecated": false,
  "description": "The website of Vinetos",
  "devDependencies": {
    "autoprefixer": "^9.8.6",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015-ie": "^6.7.0",
    "bulma": "^0.9.2",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.2"
  },
  "homepage": "https://github.com/Vinetos/Vinetos.fr#readme",
  "keywords": [
    "sass",
    "bulma",
    "css",
    "start"
  ],
  "license": "MIT",
  "name": "vinetos.fr",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vinetos/Vinetos.fr.git"
  },
  "scripts": {
    "css-build": "node-sass _sass/main.scss css/main.css",
    "css-deploy": "npm run css-build && npm run css-postcss",
    "css-postcss": "postcss --use autoprefixer --output css/main.css css/main.css",
    "css-watch": "npm run css-build -- --watch",
    "deploy": "npm run css-deploy && npm run js-build",
    "js-build": "babel _javascript --out-dir lib",
    "js-watch": "npm run js-build -- --watch",
    "start": "npm-run-all --parallel css-watch js-watch"
  },
  "version": "0.0.1",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "dependencies": {}
}
